{
    "manifest_version": 3,
    "name": "Youtube Skipper",
    "version": "1.0",
    "description": "Lightweight automatic Youtube Ad skip and also removes the annoying popup saying to disable your AdBlocker.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "*://*.youtube.com/*"
            ]
        }
    ]
  }